   ],
   "source": [
    "for i in addresses.index:\n",
    "    try:\n",
    "        #tries fetch address from geopy\n",
    "        location = geolocator.geocode(addresses['Address'][i])\n",
    "        \n",
    "        #append lat/long to column using dataframe location\n",
    "        addresses.loc[i,'location_lat'] = location.latitude\n",
    "        addresses.loc[i,'location_long'] = location.longitude\n",
    "        addresses.loc[i,'location_address'] = location.address\n",
    "    except:\n",
    "        #catches exception for the case where no value is returned\n",
    "        #appends null value to column\n",
    "        addresses.loc[i,'location_lat'] = \"\"\n",
    "        addresses.loc[i,'location_long'] = \"\"\n",
    "        addresses.loc[i,'location_address'] = \"\"\n",
    "\n",
    "#print first rows as sample\n",
    "addresses.head()"
   ]
